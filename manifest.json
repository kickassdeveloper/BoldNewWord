{
  "manifest_version": 3,
  "name": "Bold New Word",
  "version": "1.1",
  "description": "Bolds the first 50% of each word in the page",
  "permissions":["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon64.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon64.png",
    "128": "icon128.png"
  }
}

